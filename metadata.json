{
  "name": "jhoblitt-ganglia",
  "version": "5.0.0",
  "author": "Joshua Hoblitt <josh@hoblitt.com>",
  "summary": "Manages ganglia gmond & gmetad daemons + web front end",
  "license": "Apache-2.0",
  "source": "https://github.com/jhoblitt/puppet-ganglia.git",
